<idea-plugin>
  <id>cn.com.lasong.plugin.idea</id>
  <name>AndroidZApk</name>
  <version>1.0-SNAPSHOT</version>
  <vendor email="song.zhu@lasong.com.cn" url="http://www.lasong.com.cn">猪猪屋</vendor>
  <description>
      &lt;h1&gt;AndroidZDex&lt;/h1&gt;
      &lt;br/&gt;
      &lt;p&gt;&lt;b&gt;A plugin for decompile apk by apktool.&lt;/b&gt;&lt;/p&gt;
      &lt;br/&gt;
      &lt;p&gt;&lt;b&gt;集成反编译与修改jar包等多种功能为一体, 简化一些反向工程的操作&lt;/b&gt;&lt;/p&gt;
    </description>
  <change-notes>
      &lt;h1&gt; Release Note &lt;/h1&gt;
      &lt;br/&gt;
      &lt;p&gt;&lt;b&gt;A plugin for reverse engineering by apktool &amp; javassist &amp; jd-core.&lt;/b&gt;&lt;/p&gt;
      &lt;br/&gt;
      &lt;ul&gt;
        &lt;li&gt;v1.0 init&lt;/li&gt;
      &lt;/ul&gt;</change-notes>
  <idea-version since-build="202.6948" until-build="202.*"/>
  <depends>com.intellij.modules.platform</depends>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <group id="cn.com.lasong.plugin.idea.GroupedActions" text="AndroidZer" popup="true" class="cn.com.lasong.plugin.idea.AndroidZActionsGroup">
      <add-to-group group-id="ProjectViewPopupMenu" anchor="after" relative-to-action="ReplaceInPath"/>
      <action id="AndroidZer.Apk2SmaliAction" class="cn.com.lasong.plugin.idea.apk2smali.Apk2SmaliAction" text="Apk2Smali" description="Decompile apk as Smali Project"/>
      <action id="AndroidZer.JarModify" class="cn.com.lasong.plugin.idea.jar.JarModifyAction" text="JarModify" description="Modify jar by javassist"/>
    </group>
  </actions>
</idea-plugin>
